			<xs:enumeration value="4"/>  <!-- 4   CONCORRÊNCIA --> 
			<xs:enumeration value="7"/>  <!-- 7   PREGÃO --> 
			<xs:enumeration value="8"/>  <!-- 8   BEC-BOLSA ELETRÔNICA DE COMPRAS --> 
			<xs:enumeration value="10"/> <!-- 10  RDC --> 
		</xs:restriction>
	</xs:simpleType>
	<!-- ======================================================================== -->
	<!--  Alteracao prevista para a primeira versao de 2008   -->

	<xs:simpleType name="DespesaSemLicitacao_t">
		<xs:restriction base="xs:string">
			<xs:enumeration value="5"/> <!-- 5  DISPENSA DE LICITAÇÃO --> 
			<xs:enumeration value="6"/> <!-- 6  INEXIGÍVEL --> 
			<xs:enumeration value="9"/> <!-- 9  OUTROS/NÃO APLICÁVEL --> 
		</xs:restriction>
	</xs:simpleType>

	<!--  ======================================================================== -->
	<xs:simpleType name="NumeroLicitacao_t">
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Za-z0-9]{1,20}/[0-9]{4}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--  ======================================================================== -->

	<xs:complexType name="DespesaComLicitacao_t">
		<xs:sequence>
			<xs:element name="CodigoComLicitacao" type="aux:CodigoComLicitacao_t"/>
			<xs:element name="NumeroLicitacao" type="aux:NumeroLicitacao_t"/>
		</xs:sequence>
	</xs:complexType>

	<!--======================================================================== -->
	<!-- 22.2.	TABELA AUXILIAR:	TIPO DE CONVÊNIO    -->
	<xs:simpleType name="CodigoTipoConvenio_t">
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/> <!-- 1  AUXÍLIO-CONCESSÃO --> 
			<xs:enumeration value="2"/> <!-- 2  SUBVENÇÃO-CONCESSÃO --> 
			<xs:enumeration value="3"/> <!-- 3  CONTRIBUIÇÃO-CONCESSÃO --> 
			<xs:enumeration value="4"/> <!-- 4  AUXÍLIO-RECEBIDO --> 
			<xs:enumeration value="5"/> <!-- 5  SUBVENÇÃO-RECEBIDO --> 
			<xs:enumeration value="6"/> <!-- 6  CONTRIBUIÇÃO-RECEBIDO --> 
			<xs:enumeration value="9"/> <!-- 9  OUTROS --> 
		</xs:restriction>
	</xs:simpleType>
	<!-- ======================================================================== -->
	<!-- 22.3.	TABELA AUXILIAR:	TIPO DE LEGISLAÇÃO
			01	LEI FEDERAL
			02	LEI ESTADUAL
			03	LEI MUNICIPAL
			04	DECRETO FEDERAL
			05	DECRETO ESTADUAL
			06	DECRETO MUNICIPAL  
	-->
	<xs:simpleType name="CodigoTipoLegislacao_t">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ======================================================================== -->
	<xs:simpleType name="CodigoTipoLegislacaoMunicipal_t">
		<xs:restriction base="xs:string">
			<xs:enumeration value="03"/>
			<xs:enumeration value="06"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ======================================================================== -->
	<!-- 25.2.	TABELA AUXILIAR:	TIPO DE CONTRATAÇÃO  -->

	<xs:simpleType name="CodigoTipoContratacao_t">
		<xs:restriction base="xs:integer">
			<xs:enumeration value="1"/> <!-- 1  FORNECIMENTO DE MATERIAL --> 
			<xs:enumeration value="2"/> <!-- 2  FORNECIMENTO DE SERVIÇOS --> 
			<xs:enumeration value="3"/> <!-- 3  OBRAS --> 
			<xs:enumeration value="4"/> <!-- 4  LOCAÇÃO --> 
			<xs:enumeration value="5"/> <!-- 5  SERVIÇOS DE UTILIDADE PÚBLICA --> 
			<xs:enumeration value="6"/> <!-- 6  CONTRATO DE GESTÃO --> 
			<xs:enumeration value="7"/> <!-- 7  TERMO DE PARCERIA --> 
			<xs:enumeration value="8"/> <!-- 8  PARCERIA PÚBLICO PRIVADA --> 
			<xs:enumeration value="9"/> <!-- 9  OPERAÇÕES DE CRÉDITO --> 
			<xs:enumeration value="10"/> <!-- 10  CONSÓRCIOS --> 
			<xs:enumeration value="11"/> <!-- 11  CONCESSÃO --> 
			<xs:enumeration value="12"/> <!-- 12  PERMISSÃO --> 
			<xs:enumeration value="13"/> <!-- 13  FORNECIMENTO MATERIAL E SERVIÇOS --> 
			<xs:enumeration value="99"/> <!-- 99  OUTROS --> 
		</xs:restriction>
	</xs:simpleType>
 
	<!-- ======================================================================== -->
	<!-- 34.1.	TABELA AUXILIAR:	CÓDIGO DE CONTRIBUIÇÃO
			01	PESSOAL ATIVO CIVIL
			02	PESSOAL INATIVO CIVIL
			03	PESSOAL PENSIONISTA CIVIL
			04	COBERTURA DE DÉFICITS
			05	OUTROS APORTES                 
	-->
	<xs:simpleType name="CodigoContribuicao_t">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ======================================================================== -->
	<!-- 34.2.	TABELA AUXILIAR:	EXERCÍCIO DE COMPETÊNCIA      -->
	<xs:simpleType name="CodigoExercicioCompetencia_t">
		<xs:restriction base="xs:string">
			<xs:enumeration value="00"/> <!-- 00  EXERCÍCIO ATUAL --> 
			<xs:enumeration value="01"/> <!-- 01  EXERCÍCIOS ANTERIORES --> 
		</xs:restriction>
	</xs:simpleType>
	<!-- ======================================================================== -->
	<xs:simpleType name="NumeroLegislacao_t">
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Za-z0-9]{1,20}/[0-9]{4}"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ======================================================================== -->
	<xs:complexType name="Legislacao_t">
		<xs:sequence>
			<xs:element name="Numero" type="aux:NumeroLegislacao_t"/>
		</xs:sequence>
		<xs:attribute name="Tipo" type="aux:CodigoTipoLegislacao_t" use="required"/>
	</xs:complexType>
	<!-- ======================================================================== -->
	<!-- Legislacao Municipal -->
	<xs:complexType name="LegislacaoMunicipal_t">
		<xs:sequence>
			<xs:element name="Numero" type="aux:NumeroLegislacao_t"/>
		</xs:sequence>
		<xs:attribute name="Tipo" type="aux:CodigoTipoLegislacaoMunicipal_t" use="required"/>
	</xs:complexType>
	<!-- ======================================================================== -->
	<xs:simpleType name="DataLei_t"> 
		<xs:restriction base="xs:date">
			<xs:minInclusive value="2000-01-01"/>
			<xs:maxInclusive value="2016-12-31"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Legislacao Municipal com Datas -->
	<xs:complexType name="LegislacaoMunicipalComDatas_t">
		<xs:complexContent>
			<xs:extension base="aux:LegislacaoMunicipal_t">
				<xs:sequence>
					<xs:element name="DataPublicacao" type="aux:DataLei_t"/>
					<xs:element name="DataVigencia" type="aux:DataLei_t"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ======================================================================== -->

</xs:schema>